---
interface Props {
  title: string;
}
const { title } = Astro.props;
---

<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="icon" href="/assets/favicon.ico" />
    <title>{title}</title>
    <style>  @import url("https://fonts.googleapis.com/css2?family=Lexend+Deca:wght@400;700&display=swap");
  @import url("https://fonts.googleapis.com/css2?family=Lexend:wght@400;500;700&display=swap");
  @import url("https://fonts.googleapis.com/css?family=Roboto+Mono:100");</style>
  </head>

  <body>
    <script src="/app.js" is:inline></script>
    <slot />
  </body>
</html>
